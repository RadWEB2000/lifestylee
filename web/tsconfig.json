{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/anims/*": [
        "./src/lib/animations/*"
      ],
      "@/app/*": [
        "./src/app/*"
      ],
      "@/assets/*": [
        "./src/assets/*"
      ],
      "@/blocks/*": [
        "./src/components/Utils/Blocks/*"
      ],
      "@/clients/*": [
        "./src/lib/clients/*"
      ],
      "@/context/*": [
        "./src/context/*"
      ],
      "@/configs/*": [
        "./src/lib/configs/*"
      ],
      "@/css/*": [
        "./src/styles/*"
      ],
      "@/data/*": [
        "./src/data/*"
      ],
      "@/footer/*": [
        "./src/components/Layout/Footer/*"
      ],
      "@/func/*": [
        "./src/lib/functions/*"
      ],
      "@/hooks/*": [
        "./src/lib/hooks/*"
      ],
      "@/ico/*": [
        "./src/assets/icons/*"
      ],
      "@/img/*": [
        "./src/assets/images/*"
      ],
      "@/layout/*": [
        "./src/components/Layout/*"
      ],
      "@/lib/*": [
        "./src/lib/*"
      ],
      "@/nav/*": [
        "./src/components/Layout/Navigation/*"
      ],
      "@/parts/*": [
        "./src/components/*"
      ],
      "@/provider/*": [
        "./src/provider/*"
      ],
      "@/queries/*": [
        "./src/data/graphql/*"
      ],
      "@/seo/*": [
        "./src/components/Layout/Seo/*"
      ],
      "@/static/*": [
        "./src/data/static/*"
      ],
      "@/subdomains/*": [
        "./src/components/Layout/Subdomains/*"
      ],
      "@/ts/*": [
        "./src/types/*"
      ],
      "@/ui/*": [
        "./src/components/UI/*"
      ],
      "@/utils/*": [
        "./src/components/Utils/*"
      ],
      "@/videos/*": [
        "./src/assets/videos/*"
      ],
      "@/views/*": [
        "./src/components/Views/*"
      ],
      "@/v-article/*": [
        "./src/components/Views/ArticleView/*"
      ],
      "@/v-category/*": [
        "./src/components/Views/CategoryView/*"
      ],
      "@/v-contact/*": [
        "./src/components/Views/ContactView/*"
      ],
      "@/v-home/*": [
        "./src/components/Views/HomeView/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
